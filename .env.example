# Frontend Environment Variables
# Copy this file to .env.local for local development

# Backend API Base URL (REQUIRED)
# Production: https://api.abbaslogic.com
# Local: http://localhost:5000
REACT_APP_API_URL=https://api.abbaslogic.com
REACT_APP_SOCKET_URL=https://api.abbaslogic.com

# Agora Configuration (REQUIRED for Chat)
# Get these from console.agora.io
REACT_APP_AGORA_CHAT_APP_KEY=your_agora_chat_app_key
